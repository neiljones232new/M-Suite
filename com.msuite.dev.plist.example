<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.msuite.dev</string>
	
	<key>ProgramArguments</key>
	<array>
		<string>/bin/zsh</string>
		<string>-c</string>
		<string>cd "$HOME/Developer/M_Suite" &amp;&amp; source ~/.zshrc &amp;&amp; pnpm suite</string>
	</array>
	
	<key>WorkingDirectory</key>
	<string>$HOME/Developer/M_Suite</string>
	
	<key>RunAtLoad</key>
	<false/>
	
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	
	<key>ThrottleInterval</key>
	<integer>5</integer>
	
	<key>StandardOutPath</key>
	<string>/tmp/msuite.log</string>
	
	<key>StandardErrorPath</key>
	<string>/tmp/msuite.log</string>
	
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$HOME/.nvm/versions/node/v22.22.0/bin</string>
	</dict>
</dict>
</plist>
